#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jake/sensoteq-technical-test/node_modules/.pnpm/jest-cli@30.0.5_@types+node@24.3.0_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2_/node_modules/jest-cli/bin/node_modules:/Users/jake/sensoteq-technical-test/node_modules/.pnpm/jest-cli@30.0.5_@types+node@24.3.0_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2_/node_modules/jest-cli/node_modules:/Users/jake/sensoteq-technical-test/node_modules/.pnpm/jest-cli@30.0.5_@types+node@24.3.0_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2_/node_modules:/Users/jake/sensoteq-technical-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jake/sensoteq-technical-test/node_modules/.pnpm/jest-cli@30.0.5_@types+node@24.3.0_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2_/node_modules/jest-cli/bin/node_modules:/Users/jake/sensoteq-technical-test/node_modules/.pnpm/jest-cli@30.0.5_@types+node@24.3.0_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2_/node_modules/jest-cli/node_modules:/Users/jake/sensoteq-technical-test/node_modules/.pnpm/jest-cli@30.0.5_@types+node@24.3.0_ts-node@10.9.2_@types+node@24.3.0_typescript@5.9.2_/node_modules:/Users/jake/sensoteq-technical-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest-cli/bin/jest.js" "$@"
fi
